var results = db.hmda_lar.aggregate( [ {$group: {_id: { state_code: "$89", msamd: "$c0", census_tract_number: "$7c"  }}  } ] )

print ('state_code,msamd,census_tract_number')

results.result.forEach(
  function(doc){  
    var tmp = doc["_id"];  
    print( tmp["state_code"] + ',' + tmp["msamd"] + ',' + tmp["census_tract_number"] );  
  }		
)
