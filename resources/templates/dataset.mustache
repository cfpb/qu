<h1>Dataset:
  <a class="index" href="/data">/data</a>/{{dataset}}
</h1>
{{#resource.properties}}
  <h2>Properties</h2>
  <dl>
    {{#name}}
      <dt>Name</dt>
      <dd>{{name}}</dd>
    {{/name}}
    {{#description}}
      <dt>Description</dt>
      <dd>{{description}}</dd>
    {{/description}}
    {{#url}}
      <dt>URL</dt>
      <dd>
        <a href="{{url}}">{{url}}</a>
      </dd>
    {{/url}}
  </dl>
{{/resource.properties}}
<h2>Slices</h2>
<ul id="slices">
  {{#slices}}
  <li>
    <a href="{{href}}">{{properties.name}}</a>
    <small>{{properties.info.description}}</small>
  </li>
  {{/slices}}
</ul>


